{
  "name": "virtual-lawyer",
  "version": "1.0.0",
  "main": "server-combined.js",
  "type": "module",
  "scripts": {
    "start": "node server-combined.js",
    "dev": "nodemon server-combined.js",
    "build": "node build-deploy.js",
    "backend": "node index.js",
    "frontend": "cd frontend && npm start",
    "dev:both": "concurrently \"npm run backend\" \"npm run frontend\"",
    "test:imports": "node test-imports.js",
    "test:app": "node test-app.js",
    "test:env": "node test-env.js",
    "test:claude": "node test-claude.js",
    "test:claude:simple": "node test-claude-simple.cjs",
    "test:connection": "node test-connection.js",
    "check-deps": "node check-dependencies.js",
    "prestart": "node check-dependencies.js",
    "deploy:check": "node check-deployment.js",
    "deploy:vps": "bash deploy.sh",
    "deploy:docker": "bash deploy-docker.sh",
    "deploy:heroku": "deploy-heroku.bat",
    "deploy:netlify": "deploy-netlify.bat",
    "deploy:vercel": "deploy-vercel.bat",
    "deploy:render": "deploy-render.bat",
    "deploy:ftp": "deploy-ftp.bat",
    "deploy:live": "bash deploy-live.sh",
    "server:connect": "bash connect-to-server.sh",
    "prepare:deploy": "bash prepare-deployment.sh",
    "fix:frontend": "cd frontend && npm install && npm install react-scripts --save",
    "start:app": "concurrently \"npm run backend\" \"npm run frontend\"",
    "install:all": "npm install && cd frontend && npm install && cd ..",
    "build:frontend": "cd frontend && npm run build",
    "build:deploy": "node build-for-deployment.js",
    "connect:deploy": "bash connect-and-deploy.sh",
    "prebuild": "npm run check-deps",
    "deploy:simple": "node deploy-simple.js"
